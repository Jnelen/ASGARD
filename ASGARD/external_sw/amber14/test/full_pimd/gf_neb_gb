-O -i neb_gb.in -p neb_gb/tripep.top -c neb_gb/tripep.rst.3 -o neb_gb/pimd_bead3.out -r neb_gb/pimd_bead3.rst -x neb_gb/crd.3 -inf neb_gb/inf.3 -pimdout neb_gb.out
-O -i neb_gb.in -p neb_gb/tripep.top -c neb_gb/tripep.rst.4 -o neb_gb/pimd_bead4.out -r neb_gb/pimd_bead4.rst -x neb_gb/crd.4 -inf neb_gb/inf.4 -pimdout neb_gb.out
