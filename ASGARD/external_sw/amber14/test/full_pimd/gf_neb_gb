-O -i neb_gb.in -p neb_gb/tripep.top -c neb_gb/tripep.rst.1 -o neb_gb/pimd_bead1.out -r neb_gb/pimd_bead1.rst -x neb_gb/crd.1 -inf neb_gb/inf.1 -pimdout neb_gb.out
